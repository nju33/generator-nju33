{
  "presets": [
    ["env", {
      "browsers": ["> 3%", "last 2 versions"]
    }]
  ],
  "plugins": [
  <% if (compile) { %>
    "transform-runtime",
    "external-helpers",
  <% } else { %>
    "add-module-exports",
  <% } %>
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}
